                                    </a>
                                </li>

                                <li>
                                    <a href="/#contactUs" class="link-underline link-underline-1">
                                        <span>Contact Us</span>
                                    </a>
                                </li>
                                @if ((User.Identity.IsAuthenticated && User.IsInRole("Admin")))
                                {
                                <li>
                                    <a  class="link-underline link-underline-1" asp-page="">@sharedResource["Dashboard"] </a>
                                </li>
                                }
                                else
                                {
                                <li>
                                    <a  class="link-underline link-underline-1">@sharedResource["Delivery List"] </a>
                                </li>
                                }

                                @if (User.Identity.IsAuthenticated)
                                {
                                    <li>
                                        <a class="link-underline link-underline-1" asp-page="/Index" title="Manage"> <span> Hello @User.Identity.Name!</span></a>
                                    </li>
                                    <li>
                                        <a class="link-underline link-underline-1" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/", new { area = "" })">Logout</a>

