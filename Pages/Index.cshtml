@page
@model IndexModel
@using Microsoft.AspNetCore.Http
@using Microsoft.AspNetCore.Localization;


@{
    ViewData["Title"] = "Home page";
    Layout = "";
    
}
<style>
    .dark #copyrights, #copyrights.dark {
        color: white!important;
        background-color: #4786e5!important;
    }
</style>


<!DOCTYPE html>
<html lang="en">

    <head>
    <meta charset="UTF-8">
   
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700|Roboto:300,400,500,900&display=swap" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css"
          integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">

    <link rel="stylesheet" href="~/DemoLanding/css/fontawsome.css" type="text/css" />
    <link rel="stylesheet" href="~/DemoLanding/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/DemoLanding/css/style.css" type="text/css" />

    <link rel="stylesheet" href="~/DemoLanding/css/dark.css" type="text/css" />
    <link rel="stylesheet" href="/DemoLanding/css/font-icons.css" type="text/css" />
    <link rel="stylesheet" href="~/DemoLanding/css/animate.css" type="text/css" />
    <link rel="stylesheet" href="~/DemoLanding/css/magnific-popup.css" type="text/css" />

    <!-- Landing Demo Specific Stylesheet -->
    <link rel="stylesheet" href="~/DemoLanding/css/landing.css" type="text/css" />
    <link rel="stylesheet" href="~/DemoLanding/css/fonts.css" type="text/css" />

    <link rel="stylesheet" href="~/DemoLanding/css/custom.css" type="text/css" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" href="~/public/images/logo/pharmacylogo.jpeg">


</head>

<body class="stretched">

    <!-- Document Wrapper
    ============================================= -->
    <div id="wrapper" class="clearfix">

        <!-- Header
        ============================================= -->
        <header id="header" class="transparent-header full-header dark no-sticky" data-responsive-class="not-dark">
            <div id="header-wrap">
                <div class="container">
                    <div class="header-row">

                        <!-- Logo
                        ============================================= -->
                        <div id="logo" class="border-end-0">
                            <img src="~/Public/images/logo/Pharmacylogo.jpeg"style="padding:20px;border-radius:10px;" alt="Canvas">
                        </div><!-- #logo end -->

                        <div id="primary-menu-trigger">
                            <svg class="svg-trigger" viewBox="0 0 100 100"><path d="m 30,33 h 40 c 3.722839,0 7.5,3.126468 7.5,8.578427 0,5.451959 -2.727029,8.421573 -7.5,8.421573 h -20"></path><path d="m 30,50 h 40"></path><path d="m 70,67 h -40 c 0,0 -7.5,-0.802118 -7.5,-8.365747 0,-7.563629 7.5,-8.634253 7.5,-8.634253 h 20"></path></svg>
                        </div>

                        <!-- Primary Navigation
                        ============================================= -->
                        <nav class="primary-menu not-dark with-arrows">
                            <ul class="me-0 border-0 menu-container">

                                @if (User.Identity.IsAuthenticated)
                                {
                                    @if ((User.Identity.IsAuthenticated && User.IsInRole("Admin")))
                                    {
                                        <li class="menu-item"><a class="menu-link" asp-area="admin" asp-page="/Index" title="Manage"><div>@sharedResource["DashBoard"]</div></a></li>

                                    }
                                    else
                                    {
                                        <li class="menu-item"><a class="menu-link" asp-area="admin"  asp-page="/DeliveryOrders/DeliveryList" title="Manage"><div>@sharedResource["Delivery List"]</div></a></li>

                                    }
                                    <li class="menu-item"><a class="menu-link" asp-page="/Index" title="Manage"><div>Hello @User.Identity.Name!</div></a></li>
                                    <li class="menu-item"><a class="menu-link" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Page("/", new { area = "" })"><div>Logout</div></a></li>

                                }
                                else
                                {
                                    <li class="menu-item"><a class="menu-link" href="#"><div>Home</div></a></li>

                                    <li class="menu-item"><a class="menu-link" asp-area="Identity" asp-page="/Account/Login"><div>Login</div></a></li>

                                }


                            </ul>
                                
                          
                        </nav><!-- #primary-menu end -->

                    </div>
                </div>
            </div>
            <div class="header-wrap-clone"></div>
        </header><!-- #header end -->
        <!-- Slider
        ============================================= -->
        <section id="slider" class="slider-element dark min-vh-100 include-header" style="background-image: url('DemoLanding/images/1.svg');">
            <div class="slider-inner flex-column">

                <div class="vertical-middle">
                    <div class="container">
                        <div class="row align-items-lg-center g-0">

                            <div class="col-lg-6 col-md-6">
                                <h2 class="display-3 fw-bold text-white">IHCC.</h2>
                                <div>
                                    <a href="#" class="btn mt-2 text-dark bg-white rounded-3 px-4 py-3 nott ls0 shadow-sm"><i class="icon-apple1 me-2"></i>Get it on the App Store</a>
                                    <a href="#" class="ms-0 ms-lg-2 mt-2 btn text-dark bg-white rounded-3 px-4 py-3 nott ls0 shadow-sm"><i class="icon-google-play me-2"></i>Get it on Play Store</a>
                                </div>
                            </div>


                            <div class="col-lg-5 col-md-6 align-self-lg-end">
                                <div class="slide-imgs">
                                    <img src="~/DemoLanding/images/1-2.png" alt="Image" class="card-img">
                                    <img src="~/DemoLanding/images/1-1.png" alt="Image" class="iphone-img">
                                </div>
                            </div>

                        </div>
                    </div>
                </div>


            </div>
        </section><!-- #slider end -->
        <!-- Content
        ============================================= -->
        <!-- Footer
        ============================================= -->
        <footer id="footer" class="bg-white border-0">


            <div id="copyrights" class="bg-color dark py-5">
                <div class="container">

                    <div class="row col-mb-30">
                        <div class="col-md-6 text-center text-md-start">
                            
                            @sharedResource["Copyright © 2023.All Rights Reserved and Designed By"]<a href="http://code-ware.net/">
                                @sharedResource["CodeWare"]</a><span>
                                <img style="width:14%;padding-top:3rem" src="~/images/codeware.png" />
                            </span>
                            
                            <br>
                            <div class="copyright-links"> <a asp-page="privacy" class="text-white-50">Privacy Policy</a></div>
                        </div>

                        <div class="col-md-6 text-center text-md-end">
                            <a href="#" style="color:#4786e5" class="btn color bg-white rounded px-3 py-2 nott ls0 shadow-sm"><i class="icon-apple1 me-2"></i>App Store</a>
                            <a href="#" style="color:#4786e5" class="btn color bg-white rounded px-3 py-2 nott ls0 shadow-sm ms-2"><i class="icon-google-play me-2"></i>Play Store</a>
                        </div>
                    </div>

                </div>
            </div>

        </footer><!-- #footer end -->

    </div><!-- #wrapper end -->
    <!-- Go To Top
    ============================================= -->
    <div id="gotoTop" class="icon-angle-up"></div>

    <!-- JavaScripts
    ============================================= -->
    <script src="~/DemoLanding/js/jquery.js"></script>
    <script src="~/DemoLanding/js/plugins.min.js"></script>

    <!-- Footer Scripts
    ============================================= -->
    <script src="~/DemoLanding/js/functions.js"></script>

    <script>
        jQuery(window).on( 'pluginCarouselReady', function(){
        setTimeout( function(){
        $('.owl-stage').after('<div class="owl-stage-outer-bg"></div>');
        }, 1000 );
        });
    </script>


</body>
</html>












